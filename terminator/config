[global_config]
[keybindings]
  edit_window_title = None
  go_next = <Alt>bracketright
  go_prev = <Alt>bracketleft
  new_tab = <Alt>t
  next_tab = <Shift>Right
  prev_tab = <Shift>Left
  split_horiz = <Shift><Alt>d
  split_vert = <Alt>d
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
[profiles]
  [[default]]
    background_image = None
    scroll_on_output = False
    scrollback_infinite = True
